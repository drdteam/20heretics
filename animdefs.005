/* Animation definition for Shadowcaster textures when used in Doom, Heretic, Hexen or Strife. */

flat	SCBLOOD1	range SCBLOOD3	tics 8
flat	SCBLOOD4	range SCBLOOD6	tics 7
flat	SCGATEA1	range SCGATEA5	tics 6
flat	SCGATED1	range SCGATED3	tics 5
flat	SCLAVAA1	range SCLAVAA3	tics 8
flat	SCLAVAB1	range SCLAVAB3	tics 8
flat	SCLAVAC1	range SCLAVAC3	tics 4
flat	SCLAVAD1	range SCLAVAD3	tics 8
flat	SCSLIME1	range SCSLIME3	tics 8
flat	SCSLUDG1	range SCSLUDG3	tics 8
flat	SCSLUDG4	range SCSLUDG6	tics 8
flat	SCSLUDG7	range SCSLUDG9	tics 8
flat	SCWATER1	range SCWATER3	tics 8 
// SCWATER4 to SCWATERS is to be used in a script that empties water in an area.

texture SCBLOODA	range SCBLOODC	tics 8
texture SCBLOODD	range SCBLOODF	tics 8
texture SCBLUET1	range SCBLUET3	tics 6
texture SCFACES1	range SCFACES3	tics 9
texture SCGATEC1	range SCGATEC4	tics 5
texture SCGREEN1	range SCGREEN3	tics 8
texture SCHEADB1	range SCHEADB5	tics 6
texture SCHEADG1	range SCHEADG3	tics 6
texture SCROKWF1	range SCROKWF3	tics 4
texture SCSTWAT1	range SCSTWAT3	tics 8
texture SCVEINS1	range SCVEINS3	tics 8
texture SCWEIRD1	range SCWEIRD3	tics 8

switch	SCSWHED0  on  pic SCSWHED1	tics 0
switch	SCSWHRG0  on  pic SCSWHRG1	tics 0
switch	SCSWKAP0  on  pic SCSWKAP1	tics 0
switch	SCSWGRN0  on  pic SCSWGRN1	tics 0
switch	SCSWPRS0  on  pic SCSWPRS1	tics 0
switch	SCSWSQR0  on  pic SCSWSQR1	tics 0
switch	SCSWTRG0  on  pic SCSWTRG1	tics 0
switch	SCSWTRK0  on  pic SCSWTRK1	tics 0
